{
  "name": "Codespace con LangFlow y PostgreSQL",
  "dockerComposeFile": [
    "docker-compose.yml"
  ],
  "service": "langflow",
  "workspaceFolder": "/workspace",
  "shutdownAction": "stopCompose",
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "postCreateCommand": "pip install pandas sqlalchemy psycopg2-binary",
  "forwardPorts": [7860, 5432],
  "remoteUser": "root"
}
